{
  "name": "@webzaytsev/remnawave-sdk",
  "version": "1.2.2",
  "author": "WEBzaytsev <github.com/WEBzaytsev>",
  "homepage": "https://github.com/WEBzaytsev/remnawave-sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/WEBzaytsev/remnawave-sdk"
  },
  "description": "SDK for Remnawave API",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup",
    "prepublishOnly": "bun run build",
    "lint": "bun x @biomejs/biome check",
    "lint:fix": "bun run lint --fix",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "remnawave",
    "sdk",
    "api"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@types/bun": "latest",
    "tsup": "^8.5.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@remnawave/backend-contract": "2.2.32",
    "axios": "^1.13.1",
    "zod": "^4.1.12"
  }
}
